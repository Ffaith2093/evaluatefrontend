<teacherTab tabs="{{tabs}}" binditemChange="handleItemChange"></teacherTab>
<view class="all">
  <view class="myTopBack">
    <image class="back1" src="{{backPicture}}"></image>
    <view class="middleBlock">
      <image src="{{userProfile}}" class="myProfile"></image>
      <view class="centerText">
        <view class="nameText">{{userName}}</view>
        <view class="identityImg">教师</view>
      </view>
      <view class="infoBlock">
        <view class="leftBlock">
          <image class="schoolImg" src="{{schoolImg}}"></image>
          <text class="schoolTitle">学校</text>
          <text class="schoolName">陕西师范大学</text>
        </view>
        <view class="centerLine"></view>
        <view class="rightBlock">
          <image class="schoolImg" src="{{numberImg}}"></image>
          <text class="schoolTitle">教师工号</text>
          <text class="schoolName">{{teacherID}}</text>
        </view>
      </view>
    </view>
  </view>
</view>