/* components/navBar/navBar.wxss */
/*自定义导航栏*/
.nav-bar {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999999;
}

.nav-bar-white {
  /*background-color: #fff 0%;*/
  background: linear-gradient(var(--mostDeepBackgroundBlue), var(--deepBackgroundBlue));
}

.navbar-title {
  position: absolute;
  height: 32px;
  line-height: 32px;
  /* width: 100%; */
  width: 320rpx;
  text-align: center;
  font-size: 14px;
  color: #000;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  left: 28%;
}
.navbar-icon-wrap {
  position: absolute;
  left: 9px;
  width: 44px;
  height: 32px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.navbar-icon {
  width: 20px;
  height: 20px;
}

.navbar-scan {
  width: 28px;
  height: 28px;
}