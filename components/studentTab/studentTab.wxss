.bottomTab{
  position: fixed;
  display: flex;
  flex-direction: row;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 140rpx;
  background: var(--componentLightgrey);
  border-radius: 40rpx 40rpx 0 0;
  z-index: 999;
}
.titleItem{
  flex:1;
  display: flex;
  flex-direction: column;
  justify-content: center; 
  align-items: center;
}
.titleItem>image{
  height: 50rpx;
  width: 50rpx;
}
.titleItem>text{
  color: #bfbfbf;
  font-size: 28rpx;
  margin-top: 5rpx;
}
.active>text{
  color: var(--componentBlue);
}
